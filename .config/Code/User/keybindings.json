// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "alt+f",
        "command": "workbench.action.quickOpen"
    },
    {
        "key": "ctrl+p",
        "command": "-workbench.action.quickOpen"
    },
    {
        "key": "alt+b",
        "command": "workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "alt+n",
        "command": "welcome.showNewFileEntries",
        "when": "editorTextFocus && vim.mode != 'Insert' && editorTextFocus"
    },
    {
        "key": "alt+c",
        "command": "workbench.action.closeActiveEditor",
        "when": "editorTextFocus && vim.mode != 'Insert' && editorTextFocus"
    },
    {
        "key": "ctrl+j",
        "command": "selectNextSuggestion",
        "when": "suggestWidgetVisible && suggestWidgetMultipleSuggestions"
    },
    {
        "key": "ctrl+k",
        "command": "selectPrevSuggestion",
        "when": "suggestWidgetVisible && suggestWidgetMultipleSuggestions"
    },
    {
        "key": "alt+g",
        "command": "workbench.view.scm",
        "when": "workbench.scm.active && vim.mode != 'Insert' && editorTextFocus"
    },
    {
        "key": "alt+v",
        "command": "workbench.action.toggleAuxiliaryBar",
        "when": "editorTextFocus && vim.mode != 'Insert' && editorTextFocus"
    },
    {
        "key": "alt+l",
        "command": "workbench.v",
        "when": "editorTextFocus && vim.mode != 'Insert' && editorTextFocus"
    },
    {
    "key": "ctrl+l",
    "command": "settings.cycle",
    "when": "editorTextFocus",
    "args": {
        "id": "relativeLineNumbers",
        "values": [
            {
                "editor.lineNumbers": "on"
            },
            {
                "editor.lineNumbers": "relative"
            }
        ]
    }
}

]